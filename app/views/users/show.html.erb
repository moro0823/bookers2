<%= render "books/list",user:@user %>

<h1>Bookes</h1>
<table>
    <thead>
    <tr>
        <th>Title</th>
        <th>Opinion</th>
    </tr>
    </thead>
<% @books.each do |book| %>

<tbody>
 <tr>
  <td><%= attachment_image_tag @user, :profile_image, :fill, 100, 100, format: "jpeg", fallback: "no_image.jpg" %></td>
  <td><%= link_to book.title,book_path(book.id) %></td>
  <td><%= book.body %></td>
 </tr>
</tbody>
<% end %>
</table>