<%= render "books/list",user:@user %>

<h1>Users</h1>
<table>
    <thead>
    <tr>
        <th>image</th>
        <th>name</th>
    </tr>
    </thead>
<% @users.each do |user| %>
<tbody>
 <tr>
  <div>
   <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: "jpeg", fallback: "no_image.jpg" %></td>
   <td><%= user.name %></td>
   <td><%= link_to"show", user_path(user) %></td>
  </div>
 </tr>
</tbody>
<% end %>
</table>